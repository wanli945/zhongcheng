{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "setupFiles": [
      "<rootDir>/test/unit/setup"
    ],
    "coverageDirectory": "<rootDir>/test/unit/coverage"
  },
  "dependencies": {
    "@tinymce/tinymce-vue": "^4.0.0",
    "@types/axios": "^0.14.0",
    "animate.css": "^4.1.1",
    "axios": "^0.20.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.6.5",
    "element-ui": "^2.13.2",
    "fnv-plus": "^1.3.1",
    "helmet": "^4.1.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "multer": "^1.4.2",
    "qs": "^6.9.6",
    "tinymce-vue": "^1.0.0",
    "v-charts": "^1.19.0",
    "v-viewer": "^1.5.1",
    "vue-contextmenujs": "^1.3.13",
    "vue-echarts": "^5.0.0-beta.0",
    "vue-jest": "^3.0.7"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/echarts": "^4.8.0",
    "@types/express-ws-routes": "^1.1.1",
    "@types/mysql": "^2.15.15",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-webdriverio": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/test-utils": "^1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-env": "^1.7.0",
    "chromedriver": "^87.0.2",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^5.0.0",
    "default-passive-events": "^2.0.0",
    "echarts": "^4.8.0",
    "element": "^0.1.4",
    "element-ui": "^2.13.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "hls.js": "^0.14.16",
    "html2canvas": "^1.0.0-rc.7",
    "jsdom": "^16.4.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.5.0",
